<?nsuri stm?>
<machine name="ABBAMachine" initialState="FirstA">
    <state name="FirstA"/>
    <state name="B"/>
    <state name="LastA"/>

    <transition from="FirstA" to="B" char="B"/>
    <transition from="B" to="B" char="B-Z"/>
    <transition from="B" to="LastA" char="A"/>

    <accepts text="A" />
    <accepts text="ABA" />
    <accepts text="ABZBA" />

    <rejects text="X" />
    <rejects text="AAA" />
</machine>